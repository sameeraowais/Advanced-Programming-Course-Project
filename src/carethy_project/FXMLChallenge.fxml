<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane opacity="0.84" prefHeight="500.0" prefWidth="300.0" styleClass="challeng" stylesheets="@carethy_css.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carethy_project.FXMLChallengeController">
   <center>
      <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="300.0" spacing="10.0">
         <children>
            <Text fill="WHITE" fontSmoothingType="LCD" smooth="false" strokeType="OUTSIDE" strokeWidth="0.0" text="Challenges">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Text>
            <Button fx:id="w1" mnemonicParsing="false" onAction="#w1" prefHeight="50.0" prefWidth="210.0" style="-fx-background-color: #410447;" text="1 Week" textFill="WHITE">
               <font>
                  <Font name="Bookshelf Symbol 7" size="18.0" />
               </font>
            </Button>
            <Button fx:id="w2" mnemonicParsing="false" onAction="#w2" prefHeight="50.0" prefWidth="210.0" style="-fx-background-color: #410447;" text="2 Weeks" textFill="WHITE">
               <font>
                  <Font name="Bookshelf Symbol 7" size="18.0" />
               </font>
            </Button>
            <Button fx:id="w3" mnemonicParsing="false" onAction="#w3" prefHeight="50.0" prefWidth="210.0" style="-fx-background-color: #410447;" text="3 Weeks" textFill="WHITE">
               <font>
                  <Font name="Bookshelf Symbol 7" size="18.0" />
               </font>
            </Button>
            <Button fx:id="month" mnemonicParsing="false" onAction="#month" prefHeight="50.0" prefWidth="210.0" style="-fx-background-color: #410447;" text="Month" textFill="WHITE">
               <font>
                  <Font name="Bookshelf Symbol 7" size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </center>
</BorderPane>
